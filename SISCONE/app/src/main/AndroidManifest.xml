        </activity>
        <activity android:name=".RegistroProfesor" />
        <activity android:name=".RegistroRepresentante" />
        <activity android:name=".EnvioTareas" />
        <activity android:name=".EnvioComunicados" />
        <activity android:name=".AdminMaterias" />
        <activity android:name=".MenuProfesor" />
        <activity android:name=".RegistroCurso" />
        <activity android:name=".MenuCurso" />
        <activity android:name=".MenuRepresentante"></activity>
    </application>

</manifest>
