<?xml version="1.0" encoding="utf-8"?>
<AbsoluteLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent" android:layout_height="wrap_content">

    <TextView
        android:layout_width="260dp"
        android:layout_height="wrap_content"
        android:text="Cédula :"
        android:id="@+id/textView4"
        android:layout_x="180dp"
        android:layout_y="100dp"
        android:textSize="25dp" />

    <EditText
        android:layout_width="250dp"
        android:layout_height="wrap_content"
        android:inputType="number"
        android:ems="10"
        android:id="@+id/editTextCedulaProf"
        android:layout_x="450dp"
        android:layout_y="90dp" />

    <TextView
        android:layout_width="500dp"
        android:layout_height="wrap_content"
        android:text="FORMULARIO REGISTRO"
        android:id="@+id/textView5"
        android:layout_x="200dp"
        android:layout_y="35dp"
        android:textSize="30dp"
        android:textAlignment="center"
        android:textColor="#58adcf" />

    <TextView
        android:layout_width="260dp"
        android:layout_height="wrap_content"
        android:text="Apellido :"
        android:id="@+id/textView6"
        android:layout_x="180dp"
        android:layout_y="180dp"
        android:textSize="25dp" />

    <TextView
        android:layout_width="260dp"
        android:layout_height="wrap_content"
        android:text="Nombre :"
        android:id="@+id/textView7"
        android:layout_x="180dp"
        android:layout_y="140dp"
        android:textSize="25dp" />

    <EditText
        android:layout_width="250dp"
        android:layout_height="wrap_content"
        android:inputType="textPersonName"
        android:ems="10"
        android:id="@+id/editNombreProf"
        android:layout_x="450dp"
        android:layout_y="130dp" />

    <EditText
        android:layout_width="250dp"
        android:layout_height="wrap_content"
        android:inputType="textPersonName"
        android:ems="10"
        android:id="@+id/editApellidoProf"
        android:layout_x="450dp"
        android:layout_y="170dp" />

    <TextView
        android:layout_width="260dp"
        android:layout_height="wrap_content"
        android:text="Correo :"
        android:id="@+id/textView8"
        android:layout_x="180dp"
        android:layout_y="220dp"
        android:textSize="25dp" />

    <EditText
        android:layout_width="250dp"
        android:layout_height="wrap_content"
        android:inputType="textEmailAddress"
        android:ems="10"
        android:id="@+id/editEmail"
        android:layout_x="450dp"
        android:layout_y="210dp" />

    <TextView
        android:layout_width="260dp"
        android:layout_height="wrap_content"
        android:text="Contraseña :"
        android:id="@+id/textView9"
        android:layout_x="180dp"
        android:layout_y="260dp"
        android:textSize="25dp" />

    <EditText
        android:layout_width="250dp"
        android:layout_height="wrap_content"
        android:inputType="textPassword"
        android:ems="10"
        android:id="@+id/editContraseñaRegProf1"
        android:layout_x="450dp"
        android:layout_y="250dp" />

    <TextView
        android:layout_width="260dp"
        android:layout_height="wrap_content"
        android:text="Confirmar contraseña :"
        android:id="@+id/textView10"
        android:layout_x="180dp"
        android:layout_y="300dp"
        android:textSize="25dp" />

    <EditText
        android:layout_width="250dp"
        android:layout_height="wrap_content"
        android:inputType="textPassword"
        android:ems="10"
        android:id="@+id/editContraseñaRegProf2"
        android:layout_x="450dp"
        android:layout_y="290dp" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="REGISTRARSE"
        android:id="@+id/buttonIngresarRegProf"
        android:layout_x="175dp"
        android:layout_y="360dp"
        android:textSize="24dp" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="LIMPIAR"
        android:id="@+id/buttonLimpiarForRegisProf"
        android:layout_x="390dp"
        android:layout_y="360dp"
        android:textSize="24dp" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="REGRESAR LOGIN"
        android:id="@+id/buttonRegresarLoginProf"
        android:layout_x="540dp"
        android:layout_y="360dp"
        android:textSize="24dp" />

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/imageView3"
        android:layout_x="816dp"
        android:layout_y="28dp"
        android:background="@mipmap/logo" />

</AbsoluteLayout>